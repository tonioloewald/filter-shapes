{
  "name": "filter-shapes",
  "version": "1.0.0",
  "description": "filter objects to save bandwidth and catch errors, powered by type-by-example",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "build": "rm -rf dist && bun ts-standard --fix src/*.ts && bun wiptest && npx parcel build"
  },
  "keywords": [
    "javascript",
    "front-end",
    "back-end",
    "object",
    "shaps",
    "bandwidth",
    "type-checking"
  ],
  "author": "Tonio Loewald",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/filter-shapes.git"
  },
  "peerDependencies": {
    "type-by-example": "^1.0.3"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "parcel": "latest",
    "ts-standard": "^12.0.1",
    "typescript": "^5.0.4"
  },
  "targets": {
    "dist": {
      "distDir": "dist",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "includeNodeModules": {
        "type-by-example": false
      }
    },
    "cdn": {
      "distDir": "cdn",
      "optimize": true,
      "isLibrary": true,
      "outputFormat": "esmodule"
    }
  }
}
